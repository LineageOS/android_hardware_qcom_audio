    <ctl name="Voip_Tx Mixer AFE_PCM_TX_Voip" value="0" />
    <!-- compress-voip-call end-->

    <!-- split a2dp -->
    <ctl name="BT SampleRate" value="KHZ_8" />
    <ctl name="AFE Input Channels" value="Zero" />
    <ctl name="SLIM7_RX ADM Channels" value="Zero" />
    <!-- split a2dp end-->

